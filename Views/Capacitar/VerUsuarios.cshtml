@model IEnumerable<Inluirme_Proyecto.Models.Usuarios>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>VerUsuarios</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body>

    <a href="@Url.Action("Capacitaciones","Capacitar", new { id = ViewBag.id })" class="nav-link">Volver</a>
    <div>

        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Nombre</th>
                    <th scope="col">Apellido</th>
                    <th scope="col">Telefono</th>
                    <th scope="col">Discapacidad</th>
                   <th scope="col">Conocimiento</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.nombre</td>
                        <td>@item.apellido</td>
                        <td>@item.telefono</td>
                        <td>@item.Discapacidad</td>
                        <td>@item.Conocimiento</td>
                    </tr>
                }
            </tbody>
        </table>

                
    </div>
</body>
</html>
